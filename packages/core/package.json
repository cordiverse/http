{
  "name": "@cordisjs/plugin-http",
  "description": "Fetch-based axios-style HTTP client",
  "version": "1.0.0-alpha.2",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./adapter": {
      "types": "./lib/adapter/browser.d.ts",
      "node": "./lib/adapter/node.js",
      "default": "./lib/adapter/browser.js"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cordiverse/http.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/cordiverse/http/issues"
  },
  "homepage": "https://github.com/cordiverse/http",
  "keywords": [
    "http",
    "fetch",
    "axios",
    "https",
    "undici",
    "client",
    "request",
    "cordis",
    "plugin"
  ],
  "cordis": {
    "service": {
      "implements": [
        "http"
      ]
    }
  },
  "peerDependencies": {
    "cordis": "^4.0.0-alpha.4",
    "undici": "^6.21.0"
  },
  "peerDependenciesMeta": {
    "undici": {
      "optional": true
    }
  },
  "devDependencies": {
    "cordis": "^4.0.0-alpha.4",
    "undici": "^6.21.0"
  },
  "dependencies": {
    "@cordisjs/fetch-file": "^1.0.0",
    "cosmokit": "^1.7.3"
  }
}
